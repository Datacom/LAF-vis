{
  "name": "LAF-vis",
  "version": "1.0.0",
  "description": "LAF vis",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch serve",
    "serve": "sleep 1 && browser-sync start --files 'app/*.html, app/bundle.css, app/bundle.js' --server app",
    "deploy": "gh-pages -d dist",
    "build": "npm-run-all --parallel build:*",
    "build:js": "browserify -e app/js/main.js -o app/bundle.js",
    "build:css": "cssimport app/css/main.css > app/bundle.css",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "watchify app/js/main.js -o 'exorcist app/bundle.js.map > app/bundle.js' -dv",
    "watch:css": "watch -p 'app/css/main.css' -c 'npm run build:css'",
    "clean": "rm -rf dist app/bundle.css* app/bundle.js*",
    "predist": "npm run clean && npm run build",
    "dist": "mkdir dist && cp -r app/** dist && rm -rf dist/js && rm -rf dist/css"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Datacom/LAF-vis"
  },
  "author": "umbra268",
  "license": "",
  "bugs": {
    "url": "https://github.com/Datacom/LAF-vis/issues"
  },
  "homepage": "https://github.com/Datacom/LAF-vis",
  "dependencies": {
    "awesomplete": "^1.0.0",
    "bootstrap": "^3.3.6",
    "dc": "^2.0.0-beta.25",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "css-import": "0.0.1",
    "exorcist": "^0.4.0",
    "gh-pages": "^0.9.0",
    "npm-run-all": "^1.5.1",
    "watch-cli": "^0.2.1",
    "watchify": "^3.7.0"
  }
}
