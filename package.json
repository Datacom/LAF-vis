    "url": "https://github.com/Datacom/LAF-vis/issues"
  },
  "homepage": "https://github.com/Datacom/LAF-vis",
  "dependencies": {
    "awesomplete": "^1.0.0",
    "bootstrap": "^3.3.6",
    "dc": "^2.0.0-beta.25",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "css-import": "0.0.1",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
